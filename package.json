{
  "name": "auto-cask-server",
  "description": "Auto Cask Server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run mongo && concurrently \"webpack --mode=development --watch\" \"nodemon ./build/index.js\"",
    "prod": "node ./build/index.js",
    "build": "webpack --mode=production",
    "setup-mongo": "docker run --name docker-mongo  -p 27017:27017/tcp -d mongo:latest",
    "mongo": "docker start docker-mongo"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "formidable": "^3.2.5",
    "jimp": "^0.22.5",
    "jsonwebtoken": "^9.0.0",
    "mongodb-client-encryption": "^2.4.0",
    "mongoose": "^6.9.0",
    "z85": "^0.0.2",
    "zeromq": "^6.0.0-beta.16"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.16",
    "@types/formidable": "^2.0.5",
    "@types/jimp": "^0.2.28",
    "@types/jsonwebtoken": "^9.0.1",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
